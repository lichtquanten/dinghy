{
    "name": "@workspace/database",
    "version": "0.0.0",
    "type": "module",
    "private": true,
    "scripts": {
        "dev": "tsdown --watch",
        "build": "tsdown",
        "sync": "tsx src/scripts/sync.ts",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write .",
