        "@workspace/typescript-config": "workspace:*",
        "concurrently": "^9.2.1",
        "tsdown": "^0.15.8",
        "tsx": "^4.20.6",
        "typescript": "^5.9.3",
        "zod": "^4.1.12"
    },
    "exports": {
