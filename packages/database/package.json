{
    "name": "@workspace/database",
    "version": "0.0.0",
    "type": "module",
    "private": true,
    "scripts": {
        "dev": "tsdown --watch",
        "watch": "tsdown --watch",
        "build": "tsdown",
        "sync": "tsx src/scripts/sync.ts",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "typecheck": "tsc --build; tsc --build --clean",
        "preseed": "tsdown",
        "seed": "node dist/scripts/sync.js"
    },
    "dependencies": {
        "@typegoose/typegoose": "^12.20.0",
        "mongoose": "^8.19.3",
        "@workspace/code-languages": "workspace:*"
    },
    "devDependencies": {
        "@workspace/eslint-config": "workspace:*",
        "@workspace/typescript-config": "workspace:*",
        "concurrently": "^9.2.1",
        "tsdown": "^0.15.12",
        "tsx": "^4.20.6",
        "typescript": "^5.9.3",
        "zod": "^4.1.12"
    },
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    }
}
